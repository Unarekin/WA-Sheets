            <div class="col-sm-6 col-12 min-height-20">
              <input type="text" id="equipment_{{index + 8}}" name="equipment_{{index + 8}}" value="{{ variables["equipment_" ~ (index + 8)] is defined ? variables["equipment_" ~ (index + 8)] }}">
