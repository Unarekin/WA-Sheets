          {% set intensity1 = variables["intimacy_" ~ index ~ "_intensity"] is defined ? variables["intimacy_" ~ index ~ "_intensity"] : 0 %}
          {% set name2 = variables["intimacy_" ~ (index + 10) ~ "_name"] is defined ? variables["intimacy_" ~ (index + 10) ~ "_name"] : "" %}
          {% set intensity2 = variables["intimacy_" ~ (index + 10) ~ "_intensity"] is defined ? variables["intimacy_" ~ (index + 10) ~ "_intensity"] : 0 %}
          <div class="col-sm-6 col-12">
            <div class="row">
              <div class="col-8 col-sm-10">
                <input type="text" name="intimacy_{{index}}_name" id="intimacy_{{index}}_name" value="{{ name1 }}">
              </div>
              <div class="col-4 col-sm-2">
                <input type="text" id="intimacy_{{index}}_intensity" name="intimacy_{{index}}_intensity" value="{{ intensity1 }}">
              </div>
            </div>
          </div>
          <div class="col-sm-6 col-12">
            <div class="row">
              <div class="col-8 col-sm-10">
                <input type="text" name="intimacy_{{index + 10}}_name" id="intimacy_{{index + 10}}_name" value="{{ name2 }}">
              </div>
              <div class="col-4 col-sm-2">
                <input type="text" name="intimacy_{{index + 10}}_intensity" id="intimacy_{{index + 10}}_intensity" value="{{ intensity2 }}">
              </div>
            </div>
          </div>
        {% endfor %}
      </div>
      <!-- Equipment -->
      {{ sheet.separator("Equipment") }}
      <div class="row">
        <div class="col-12">
          {% for index in 0..7 %}
          <div class="row">
            <div class="col-sm-6 col-12 min-height-20">
              <input type="text" id="equipment_{{index}}" name="equipment_{{index}}" value="{{ variables["equipment_" ~ index] is defined ? variables["equipment_" ~ index] }}">
